    "mysql2": "^3.6.5",
    "web-push": "^3.6.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/web-push": "^3.6.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
